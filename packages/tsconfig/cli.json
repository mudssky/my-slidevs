{
  "compilerOptions": {
    
    "target": "ESNext", 
    "module": "CommonJS", 
    
    

    
    "esModuleInterop": true,
    "resolveJsonModule": true, 
    "moduleResolution": "node", 

    
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictBindCallApply": true,

    
    "sourceMap": true, 
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true,

    
    "types": ["node"], 
    "declaration": true 
  }
}

