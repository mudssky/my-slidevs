{
  "name": "@mudssky/slidev-style",
  "version": "0.1.0",
  "description": "custom style for slidev",
  "keywords": [
    "css",
    "slidev"
  ],
  "license": "ISC",
  "author": "mudssky",
  "exports": {
    ".": {
      "import": "./index.ts",
      "require": "./index.ts"
    },
    "./*.css": {
      "import": "./*.css",
      "require": "./*.css"
    },
    "./scss/*.scss": {
      "import": "./scss/*.scss",
      "require": "./scss/*.scss"
    }
  },
  "main": "index.ts",
  "module": "index.ts",
  "files": [
    "./*.css",
    "scss/",
    "*.ts"
  ],
  "scripts": {
    "build": "pnpm sass ./scss/index.scss ./index.css --no-source-map",
    "prepublishOnly": "pnpm build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "publishConfig": {
    "access": "public"
  }
}
