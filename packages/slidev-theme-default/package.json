{
  "name": "@mudssky/slidev-theme-default",
  "version": "0.1.0",
  "description": "my slidev theme",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "type": "module",
  "files": [
    "components",
    "setup",
    "styles",
    "index.ts"
  ],
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@mudssky/slidev-addon-default": "workspace:*",
    "@mudssky/slidev-style": "workspace:^",
    "@slidev/types": "^0.46.3",
    "codemirror-theme-vars": "^0.1.2",
    "prism-theme-vars": "^0.2.4",
    "theme-vitesse": "^0.7.5"
  },
  "devDependencies": {
    "@slidev/cli": "^0.46.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "slidev": ">=0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
  "slidev": {
    "colorSchema": "both",
    "highlighter": "all",
    "defaults": {
      "fonts": {
        "sans": "Nunito Sans",
        "mono": "Fira Code"
      },
      "hightlighter": "prism",
      "addons": [
        "@mudssky/slidev-addon-default"
      ]
    },
    "addons": [
      "@mudssky/slidev-addon-default"
    ]
  }
}
